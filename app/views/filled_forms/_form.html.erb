<%= simple_form_for [@form, @filled_form], :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.error_notification %>

  <div class='form-inputs'>
    <%= f.fields_for @filled_form.filled_fields do |ff| %>
      <%= ff.object %>
      <%= ff.input :value, label: 'as' %>
      <%# Here we will render filledfields %>
    <% end %>
  </div>
  <div class="form-actions">
    <%= f.button :submit, class: 'btn-primary' %>
    <%= link_to t('.cancel', default: t("helpers.links.cancel")),
      form_filled_forms_path(@form), class: 'btn' %>
  </div>
<% end %>
