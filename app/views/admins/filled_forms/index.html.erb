<div class="page-header">
  <h1><%=t '.title_html', form: @form.title %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= FilledForm.human_attribute_name(:id) %></th>
      <th><%= FilledForm.human_attribute_name(:user) %></th>
      <th><%= FilledForm.human_attribute_name(:state) %></th>
      <th><%= FilledForm.human_attribute_name(:created_at) %></th>
      <th><%= FilledForm.human_attribute_name(:updated_at) %></th>
      <th><%= t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @filled_forms.each do |filled_form| %>
      <tr>
        <td><%= link_to filled_form.id, admins_form_filled_form_path(@form, filled_form) %></td>
        <td><%= filled_form.user.name %></td>
        <td><%= state_label filled_form %></td>
        <td><%= localize_date filled_form.created_at, format: :short %></td>
        <td><%= localize_date filled_form.updated_at, format: :short %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit_html")),
                      edit_admins_form_filled_form_path(@form, filled_form), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy_html")),
                      admins_form_filled_form_path(@form, filled_form),
                      :method => :delete,
                      :confirm => t('.confirm', :default => t("helpers.links.confirm")),
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new_html")),
            new_admins_form_filled_form_path(@form),
            :class => 'btn btn-primary' %>
