<div class="page-header">
  <h1><%= @form.title %></h1>
</div>
<%= not_enabled_alert unless @form.enabled? %>
<%= content_tag :div, @form.description, data: { markdown: @form.description } %>
<%= render 'form_filled_forms', form: @form %>

<div class="form-actions">
  <%= fill_new_btn @form %>
  <%= link_to t('.back', default: t("helpers.links.back")),
    forms_path, :class => 'btn'  %>
</div>
